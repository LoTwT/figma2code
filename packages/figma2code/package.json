{
  "name": "figma2code",
  "version": "0.0.1",
  "packageManager": "pnpm@8.6.7",
  "description": "figma2code",
  "author": "LoTwT <l709937065@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/LoTwT/figma2code#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LoTwT/figma2code.git"
  },
  "bugs": {
    "url": "https://github.com/LoTwT/figma2code/issues"
  },
  "keywords": [
    "figma",
    "figma to code"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "watch": "tsup --watch",
    "build": "tsup",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "prepublishOnly": "pnpm build",
    "release": "bumpp && npm publish"
  },
  "devDependencies": {
    "@figma/plugin-typings": "^1.70.0",
    "@types/node": "^20.4.2",
    "@types/react": "^18.2.15",
    "bumpp": "^9.1.1",
    "pnpm": "^8.6.7",
    "react": "^18.2.0",
    "tsup": "^7.1.0",
    "typescript": "^5.1.6",
    "unplugin-auto-import": "^0.16.6",
    "vitest": "^0.33.0"
  }
}
