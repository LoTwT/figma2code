{
  "name": "figma-rest-types",
  "version": "0.0.2",
  "packageManager": "pnpm@8.6.10",
  "description": "figma reset api types",
  "author": "LoTwT <l709937065@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/LoTwT/figma2code#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LoTwT/figma2code.git"
  },
  "bugs": {
    "url": "https://github.com/LoTwT/figma2code/issues"
  },
  "keywords": [
    "figma",
    "figma reset api",
    "types"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts"
    }
  },
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "watch": "tsup --watch",
    "build": "tsup",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "pnpm build",
    "release": "bumpp && npm publish"
  },
  "devDependencies": {
    "@ayingott/sucrose": "^0.0.4",
    "bumpp": "^9.1.1",
    "tsup": "^7.1.0",
    "typescript": "^5.1.6"
  }
}
